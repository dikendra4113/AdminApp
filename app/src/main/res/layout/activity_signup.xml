<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Signup">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pattern_left" />

    <ImageView
        android:id="@+id/bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/pattern_right" />

    <TextView
        android:id="@+id/crete_account_hdg_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="#00FFFFFF"
        android:text="Create Account"
        android:textColor="#000000"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/name_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/crete_account_hdg_tv"
        app:srcCompat="@drawable/textfield_bg" />

    <ImageView
        android:id="@+id/name_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/name_bg"
        app:layout_constraintStart_toStartOf="@+id/name_bg"
        app:layout_constraintTop_toTopOf="@+id/name_bg"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/nam_ic" />

    <EditText

        android:id="@+id/fullname_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Owner Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/name_bg"
        app:layout_constraintEnd_toEndOf="@+id/name_bg"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toEndOf="@+id/name_ic"
        app:layout_constraintTop_toTopOf="@+id/name_bg"
        app:layout_constraintVertical_bias="0.409" />





    <ImageView
        android:id="@+id/email_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_bg"
        app:srcCompat="@drawable/textfield_bg" />

    <ImageView
        android:id="@+id/email_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/email_bg"
        app:layout_constraintStart_toStartOf="@+id/email_bg"
        app:layout_constraintTop_toTopOf="@+id/email_bg"
        app:layout_constraintVertical_bias="0.428"
        app:srcCompat="@drawable/mail_ic" />


    <EditText
        android:id="@+id/email_id_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Email Id"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/email_bg"
        app:layout_constraintEnd_toEndOf="@+id/email_bg"
        app:layout_constraintHorizontal_bias="0.194"
        app:layout_constraintStart_toEndOf="@+id/email_ic"
        app:layout_constraintTop_toTopOf="@+id/email_bg"
        app:layout_constraintVertical_bias="0.363" />


    <ImageView
        android:id="@+id/restaurant_name_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_bg"
        app:srcCompat="@drawable/textfield_bg" />


    <ImageView
        android:id="@+id/restaurant_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/restaurant_name_bg"
        app:layout_constraintStart_toStartOf="@+id/restaurant_name_bg"
        app:layout_constraintTop_toTopOf="@+id/restaurant_name_bg"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/restaurant_name_ic" />

    <EditText

        android:id="@+id/restaurant_name_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Restaurant Name"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/restaurant_name_bg"
        app:layout_constraintEnd_toEndOf="@+id/restaurant_name_bg"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toEndOf="@+id/restaurant_ic"
        app:layout_constraintTop_toTopOf="@+id/restaurant_name_bg"
        app:layout_constraintVertical_bias="0.409" />



    <ImageView
        android:id="@+id/phonebg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_name_bg"
        app:srcCompat="@drawable/textfield_bg" />

    <ImageView
        android:id="@+id/phone_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/phonebg"
        app:layout_constraintStart_toStartOf="@+id/phonebg"
        app:layout_constraintTop_toTopOf="@+id/phonebg"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/phone_ic" />

    <EditText
        android:id="@+id/phone_no_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Restaurant Phone Number"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="@+id/phonebg"
        app:layout_constraintEnd_toEndOf="@+id/phonebg"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toEndOf="@+id/phone_icon"
        app:layout_constraintTop_toTopOf="@+id/phonebg"
        app:layout_constraintVertical_bias="0.409" />


    <ImageView
        android:id="@+id/address_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phonebg"
        app:srcCompat="@drawable/textfield_bg" />


    <ImageView
        android:id="@+id/address_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/address_bg"
        app:layout_constraintStart_toStartOf="@+id/address_bg"
        app:layout_constraintTop_toTopOf="@+id/address_bg"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/address_ic" />

    <EditText

        android:id="@+id/address_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPostalAddress"
        app:layout_constraintBottom_toBottomOf="@+id/address_bg"
        app:layout_constraintEnd_toEndOf="@+id/address_bg"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toEndOf="@+id/address_ic"
        app:layout_constraintTop_toTopOf="@+id/address_bg"
        app:layout_constraintVertical_bias="0.409" />



    <ImageView
        android:id="@+id/city_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_bg"
        app:srcCompat="@drawable/textfield_bg" />


    <ImageView
        android:id="@+id/city_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/city_bg"
        app:layout_constraintStart_toStartOf="@+id/city_bg"
        app:layout_constraintTop_toTopOf="@+id/city_bg"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/city_ic" />

    <EditText

        android:id="@+id/city_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="City"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/city_bg"
        app:layout_constraintEnd_toEndOf="@+id/city_bg"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toEndOf="@+id/city_ic"
        app:layout_constraintTop_toTopOf="@+id/city_bg"
        app:layout_constraintVertical_bias="0.409" />


    <ImageView
        android:id="@+id/dropdown_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/city_bg"
        app:layout_constraintEnd_toEndOf="@+id/city_bg"
        app:layout_constraintTop_toTopOf="@+id/city_bg"
        app:layout_constraintVertical_bias="0.491"
        app:srcCompat="@drawable/arrow_down_ic" />




    <ImageView
        android:id="@+id/passwordbg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/city_bg"
        app:srcCompat="@drawable/textfield_bg" />

    <ImageView
        android:id="@+id/password_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/passwordbg"
        app:layout_constraintStart_toStartOf="@+id/passwordbg"
        app:layout_constraintTop_toTopOf="@+id/passwordbg"
        app:layout_constraintVertical_bias="0.408"
        app:srcCompat="@drawable/password_ic" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/passwordbg"
        app:layout_constraintEnd_toEndOf="@+id/passwordbg"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toEndOf="@+id/password_ic"
        app:layout_constraintTop_toTopOf="@+id/passwordbg"
        app:layout_constraintVertical_bias="0.363" />



    <ImageView
        android:id="@+id/password_eye"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/passwordbg"
        app:layout_constraintEnd_toEndOf="@+id/passwordbg"
        app:layout_constraintTop_toTopOf="@+id/passwordbg"
        app:layout_constraintVertical_bias="0.491"
        app:srcCompat="@drawable/eye_ic" />


    <TextView
        android:id="@+id/signuptext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:text="Sign Up"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/signinbg"
        app:layout_constraintTop_toBottomOf="@+id/passwordbg" />

    <ImageView
        android:id="@+id/signinbg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordbg"
        app:srcCompat="@drawable/yellow_btn" />

</androidx.constraintlayout.widget.ConstraintLayout>